{
    "name": "temporal/open-telemetry-interceptors",
    "description": "This package provides OpenTelemetry interceptors for Temporal PHP SDK",
    "keywords": [
        "temporal",
        "open-telemetry",
        "interceptors",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Buchnev",
            "homepage": "https://github.com/butschster"
        },
        {
            "name": "Aleksei Gagarin",
            "homepage": "https://github.com/roxblnfk"
        }
    ],
    "homepage": "https://temporal.io/",
    "support": {
        "docs": "https://docs.temporal.io/",
        "chat": "https://t.mp/slack"
    },
    "require": {
        "php": ">=8.1",
        "open-telemetry/sdk": "^1.4",
        "symfony/polyfill-php83": "^1.32",
        "temporal/sdk": "^2.14.1"
    },
    "require-dev": {
        "open-telemetry/api": "^1.3.0",
        "phpunit/phpunit": "^10.2",
        "spiral/code-style": "^2.2.2",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Temporal\\OpenTelemetry\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Temporal\\OpenTelemetry\\Tests\\": "tests\\src"
        }
    },
    "scripts": {
        "cs:diff": "php-cs-fixer fix --dry-run -v --diff --show-progress dots",
        "cs:fix": "php-cs-fixer fix -v",
        "psalm": "psalm",
        "psalm:baseline": "psalm --set-baseline=psalm-baseline.xml",
        "test": "phpunit --color=always --testdox"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false,
            "tbachert/spi": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
